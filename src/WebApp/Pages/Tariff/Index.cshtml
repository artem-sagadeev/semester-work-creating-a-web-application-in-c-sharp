@page
@model WebApp.Pages.Tariff.IndexModel
@foreach (var tariffodel in Model.TariffModels)
{
    <p>
        Id = <a asp-page="/Tariff/DetailsByTariffId" asp-route-tariffId="@tariffodel.Id">@tariffodel.Id</a>;
        SubscribedTypeId = <a asp-page="/Tariff/DetailsBySubscriptionTypeId" asp-route-subscriptionTypeId="@tariffodel.TypeOfSubscription.Id"> @tariffodel.TypeOfSubscription.Id</a>;
        @tariffodel.Name @tariffodel.PricePerMonth руб
    </p>
}

