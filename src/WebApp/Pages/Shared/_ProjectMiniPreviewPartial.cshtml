@using WebApp.Services.Files
@using WebApp.Models.Files
@model WebApp.Models.Developer.ProjectModel

@inject IFileService _fileService

@{
    var avatar = await _fileService.GetAvatar(Model.Id, CreatorType.Project);
}

<div>
    @Model.Id
    @Model.Name
    <img src="@avatar?.Path" alt="avatar">
</div>